<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.DetailUndanganActivity"
    android:background="#FFFF">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="675dp"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/orange_500"
        android:minWidth="48dp"
        android:minHeight="48dp"
        app:layout_constraintBottom_toBottomOf="@+id/detailjadwal_judulkategori"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/detailjadwal_judulkategori"
        app:srcCompat="@drawable/ic_arrow_back"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/detailjadwal_judulkategori"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_semibold"
        android:text="Detail Jadwal Seminar Hasil"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/toolbar6" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="350dp"
        android:layout_height="560dp"
        android:layout_marginTop="36dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar6"
        app:srcCompat="@drawable/form" />

    <View
        android:id="@+id/view"
        android:layout_width="300dp"
        android:layout_height="1dp"
        android:layout_marginTop="32dp"
        android:background="#EFEFEF"
        app:layout_constraintStart_toStartOf="@+id/detailjadwal_lokasi"
        app:layout_constraintTop_toBottomOf="@+id/divider10" />

    <TextView
        android:id="@+id/detailjadwal_jadwal1"
        android:layout_width="190dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/lato_regular"
        android:text="Rabu, 16 Oktober 2023 08.30 WIB"
        android:textColor="#000000"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/detailjadwal_jadwal"
        app:layout_constraintEnd_toEndOf="@+id/detailjadwal_dospa1"
        app:layout_constraintTop_toTopOf="@+id/detailjadwal_jadwal" />

    <TextView
        android:id="@+id/detailjadwal_jadwal"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/lato_regular"
        android:text="Jadwal Seminar  :"
        android:textColor="#000000"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/detailjadwal_dosbing"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/divider9"
        app:layout_constraintTop_toBottomOf="@+id/divider9" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="190dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/lato_regular"
        android:text="Ruang Seminar E"
        android:textColor="#000000"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/detailjadwal_lokasi"
        app:layout_constraintEnd_toEndOf="@+id/detailjadwal_dospa1"
        app:layout_constraintTop_toTopOf="@+id/detailjadwal_lokasi" />

    <View
        android:id="@+id/divider10"
        android:layout_width="300dp"
        android:layout_height="1dp"
        android:layout_marginTop="36dp"
        android:background="#EFEFEF"
        app:layout_constraintStart_toStartOf="@+id/detailjadwal_jadwal"
        app:layout_constraintTop_toBottomOf="@+id/divider9" />

    <View
        android:id="@+id/divider9"
        android:layout_width="300dp"
        android:layout_height="1dp"
        android:layout_marginTop="12dp"
        android:background="#EFEFEF"
        app:layout_constraintStart_toStartOf="@+id/detailjadwal_dospa"
        app:layout_constraintTop_toBottomOf="@+id/detailjadwal_dospa1" />

    <TextView
        android:id="@+id/detailjadwal_dospa1"
        android:layout_width="190dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/lato_regular"
        android:text="Dinan Yulianto S.T., M.Eng."
        android:textColor="#000000"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/detailjadwal_dospa"
        app:layout_constraintEnd_toEndOf="@+id/divider8"
        app:layout_constraintTop_toTopOf="@+id/detailjadwal_dospa" />

    <Button
        android:id="@+id/button_fileTA"
        style="@style/Widget.MaterialComponents.Button.OutlinedButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:background="@color/white"
        android:drawableLeft="@drawable/ic_file_pdf"
        android:fontFamily="@font/lato_bold_italic"
        android:minHeight="48dp"
        android:text="File Tugas Akhir"
        android:textAllCaps="false"
        android:textColor="#000000"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@+id/detailjadwal_lokasi"
        app:layout_constraintTop_toBottomOf="@+id/detailjadwal_lokasi" />

    <TextView
        android:id="@+id/detailjadwal_dospa"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/lato_regular"
        android:text="Dosen PA          :"
        android:textColor="#000000"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/detailjadwal_dosbing"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/divider8"
        app:layout_constraintTop_toBottomOf="@+id/divider8" />

    <TextView
        android:id="@+id/detailjadwal_lokasi"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/lato_regular"
        android:text="Lokasi Seminar   :"
        android:textColor="#000000"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/detailjadwal_dosbing"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/detailjadwal_jadwal"
        app:layout_constraintTop_toBottomOf="@+id/detailjadwal_jadwal" />

    <Button
        android:id="@+id/detailjadwal_btn_acc"
        style="@style/Widget.MaterialComponents.Button.OutlinedButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawableLeft="@drawable/ic_accept"
        android:fontFamily="@font/poppins_extrabold"
        android:text="Terima"
        android:textColor="#48742C"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/detailjadwal_btn_reject"
        app:layout_constraintEnd_toEndOf="@+id/textView5"
        app:layout_constraintStart_toEndOf="@+id/detailjadwal_btn_reject"
        app:layout_constraintTop_toTopOf="@+id/detailjadwal_btn_reject" />

    <View
        android:id="@+id/divider8"
        android:layout_width="300dp"
        android:layout_height="1dp"
        android:layout_marginTop="36dp"
        android:background="#EFEFEF"
        app:layout_constraintEnd_toEndOf="@+id/detailjadwal_dosbing1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/detailjadwal_dosbing"
        app:layout_constraintTop_toBottomOf="@+id/divider7" />

    <TextView
        android:id="@+id/detailjadwal_judul1"
        android:layout_width="190dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:fontFamily="@font/lato_regular"
        android:text="Sistem Pakar Diagnosa Penyakit Tanaman Padi dengan Forward dan Backward Chaining"
        android:textColor="#000000"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/detailjadwal_judul"
        app:layout_constraintEnd_toEndOf="@+id/divider6"
        app:layout_constraintStart_toEndOf="@+id/detailjadwal_judul"
        app:layout_constraintTop_toTopOf="@+id/detailjadwal_judul" />

    <View
        android:id="@+id/divider7"
        android:layout_width="300dp"
        android:layout_height="1dp"
        android:layout_marginTop="6dp"
        android:background="#EFEFEF"
        app:layout_constraintStart_toStartOf="@+id/detailjadwal_judul"
        app:layout_constraintTop_toBottomOf="@+id/detailjadwal_judul1" />

    <TextView
        android:id="@+id/detailjadwal_dosbing"
        android:layout_width="95dp"
        android:layout_height="31dp"
        android:layout_marginBottom="13dp"
        android:fontFamily="@font/lato_regular"
        android:text="Dosen Pembimbing TA   :"
        android:textColor="#000000"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@+id/divider8"
        app:layout_constraintStart_toStartOf="@+id/divider7"
        app:layout_constraintTop_toBottomOf="@+id/divider7"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/detailjadwal_nim"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="6dp"
        android:fontFamily="@font/lato_regular"
        android:text="2000018338"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/detailjadwal_nama"
        app:layout_constraintTop_toBottomOf="@+id/detailjadwal_nama" />

    <TextView
        android:id="@+id/detailjadwal_kategori"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/lato_bold"
        android:text="Seminar Hasil"
        android:textColor="#000000"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <TextView
        android:id="@+id/detailjadwal_nama"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:fontFamily="@font/lato_regular"
        android:text="Mafatihul Hidayah"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toTopOf="@+id/imageView4" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        app:layout_constraintStart_toStartOf="@+id/detailjadwal_kategori"
        app:layout_constraintTop_toBottomOf="@+id/detailjadwal_kategori"
        app:srcCompat="@drawable/ic_profile" />

    <View
        android:id="@+id/divider6"
        android:layout_width="300dp"
        android:layout_height="3dp"
        android:layout_marginTop="18dp"
        android:background="#000000"
        app:layout_constraintStart_toStartOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/imageView4" />

    <Button
        android:id="@+id/detailjadwal_btn_reject"
        style="@style/Widget.MaterialComponents.Button.OutlinedButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:drawableLeft="@drawable/ic_reject"
        android:fontFamily="@font/poppins_extrabold"
        android:text="Tolak"
        android:textColor="#8B1A10"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/button_fileTA"
        app:layout_constraintTop_toBottomOf="@+id/button_fileTA" />

    <TextView
        android:id="@+id/detailjadwal_judul"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/lato_regular"
        android:text="Judul TA                    :"
        android:textColor="#000000"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@+id/divider6"
        app:layout_constraintTop_toBottomOf="@+id/divider6" />

    <TextView
        android:id="@+id/detailjadwal_dosbing1"
        android:layout_width="190dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:fontFamily="@font/lato_regular"
        android:text="Miftahurrahma Rosyda, S.Kom, M.Eng"
        android:textColor="#000000"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/detailjadwal_dosbing"
        app:layout_constraintEnd_toEndOf="@+id/divider7"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toEndOf="@+id/detailjadwal_dosbing"
        app:layout_constraintTop_toTopOf="@+id/detailjadwal_dosbing"
        app:layout_constraintVertical_bias="1.0" />
</androidx.constraintlayout.widget.ConstraintLayout>